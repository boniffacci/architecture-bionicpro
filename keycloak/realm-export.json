        "syncMode": "INHERIT",
        "claim": "last_name",
        "user.attribute": "lastName"
      }
    },
    {
      "name": "yandex-reports-api-audience-mapper",
      "identityProviderAlias": "yandex-oauth",
      "identityProviderMapper": "oidc-audience-mapper",
      "config": {
        "included.client.audience": "reports-api",
        "included.custom.audience": ""
      }
    }
  ]

        "defaultScope": "login:info login:email",
        "clientAuthMethod": "client_secret_post",
        "pkceEnabled": "false",
        "userIDClaim": "id",
        "userNameClaim": "login",
        "emailClaim": "default_email",
        "givenNameClaim": "first_name",
        "familyNameClaim": "last_name",
        "fullNameClaim": "display_name"
      }
    }
  ],
  "identityProviderMappers": [
    {
      "name": "yandex-user-id-mapper",
      "identityProviderAlias": "yandex-oauth",
      "identityProviderMapper": "hardcoded-attribute-idp-mapper",
      "config": {
        "syncMode": "INHERIT",
        "attribute": "yandex_id",
        "attribute.value": ""
      }
    },
    {
      "name": "yandex-username-mapper",
      "identityProviderAlias": "yandex-oauth",
      "identityProviderMapper": "oidc-username-idp-mapper",
      "config": {
        "syncMode": "INHERIT",
        "template": "${CLAIM.login}"
      }
    },
    {
      "name": "yandex-email-mapper",
      "identityProviderAlias": "yandex-oauth",
      "identityProviderMapper": "oidc-user-attribute-idp-mapper",
      "config": {
        "syncMode": "INHERIT",
        "claim": "default_email",
        "user.attribute": "email"
      }
    },
    {
      "name": "yandex-firstname-mapper",
      "identityProviderAlias": "yandex-oauth",
      "identityProviderMapper": "oidc-user-attribute-idp-mapper",
      "config": {
        "syncMode": "INHERIT",
        "claim": "first_name",
        "user.attribute": "firstName"
      }
    },
    {
      "name": "yandex-lastname-mapper",
      "identityProviderAlias": "yandex-oauth",
